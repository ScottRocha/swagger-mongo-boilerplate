  "repository": {
    "type": "git",
    "url": "https://github.com/ScottRocha/swagger-mongo-boilerplate"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "production": "node ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=production node app.js",
    "staging": "node ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=staging node app.js",
    "development": "node --max_old_space_size=4096 ./node_modules/cross-env/dist/bin/cross-env NODE_ENV=development node app.js",
    "lint": "eslint api config handlers jobs app.js --quiet",
    "test": "yarn run lint"
  },
  "engines": {
    "node": ">=8.9.4",
    "yarn": ">=1.3.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "3.5.1",
    "config": "1.30.0",
    "cors": "2.8.4",
    "cron": "1.3.0",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "http-auth": "3.2.3",
    "js-yaml": "3.11.0",
    "jsonwebtoken": "8.2.1",
    "lodash": "4.17.5",
    "moment": "2.22.0",
    "mongoose": "5.13.0",
    "morgan": "1.9.0",
    "swagger-express-mw": "0.7.0",
    "swagger-ui-express": "3.0.6",
    "winston": "2.4.1"
  }
}
